	    { "type": "pathPrefix", "prefix": "/metadata", "rule": { "type": "staticService", "hosts": [{ "protocol": "http", "host": "localhost:9120"}]}},
	    { "type": "pathPrefix", "prefix": "/metrics", "rule": { "type": "staticService", "hosts": [{ "protocol": "http", "host": "localhost:9191"}]}},
